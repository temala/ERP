{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Store",
      "StoreName": "My",
      "StoreLocation": "CurrentUser",
      "Name": "CN=MyApplication"
    },
    "Clients": {
      "AngularWebUi": {
        "Profile": "SPA",
        "RedirectUri": "https://your-domain.com/authentication/login-callback",
        "LogoutUri": "https://your-domain.com/authentication/logout-callback"
      }
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [
    "https://your-domain.com"
  ]
}
